<div class="page-header">
    <h1>Feeds</h1>
</div>
<div class="row">
    <div class="col-xs-12">
        <div class="list group">
            <% @feeds.each do |feed| %>
                <%= link_to feed.title, feed, class: "list-group-item" %>
            <% end %>
        </div>
    </div>
</div>



